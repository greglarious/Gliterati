{
    "name": "Gliterati",
    "description": "library for led visual patterns",
    "keywords": "led",
    "authors": [
        {
            "name": "Greg Frank",
            "url": "https://github.com/greglarious",
            "maintainer": true
        },
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/greglarious/gliterati.git"
    },
    "version": "0.0.1",
    "license": "MIT",
    "platforms": "atmelavr, atmelsam, teensy",
    "export": {
        "exclude": [
            "docs",
            "extras"
        ]
    },
    "build": {
        "srcFilter": [
            "+<*.c>",
            "+<*.cpp>",
            "+<*.h>",
            "+<platforms/esp/32/*.cpp>"
        ],
        "libArchive": false
    }
}
